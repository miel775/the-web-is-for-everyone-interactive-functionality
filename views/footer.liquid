<section class="laatste-nieuws">
    <h2>Laatste Nieuws</h2>

    <div class="container">
        
        {% for publication in datedpublications %}
            <article href="/publication/{{ publication.id }}">
                <img src="{% if publication.photo %}https://fdnd-agency.directus.app/assets/{{ publication.photo }}{% else %}https://media.istockphoto.com/id/887464786/vector/no-cameras-allowed-sign-flat-icon-in-red-crossed-out-circle-vector.jpg?s=612x612&w=0&k=20&c=LVkPMBiZas8zxBPmhEApCv3UiYjcbYZJsO-CVQjAJeU={% endif %}" alt="publicatie-foto" width="100p">
                <p> {{ publication.topic }} </p>
            <p> {{ publication.date }} </p>

            <h4> {{ publication.title }} </h4>

            <a href="/publication/{{ publication.id }}"> Bekijk </a>
            </article>
        {% endfor %}
        
    </div>

    <div class="container-mobile">
        
    {% for publication in datedpublications %}
        <article href="/publication/{{ publication.id }}">
            <img src="{% if publication.photo %}https://fdnd-agency.directus.app/assets/{{ publication.photo }}{% else %}https://media.istockphoto.com/id/887464786/vector/no-cameras-allowed-sign-flat-icon-in-red-crossed-out-circle-vector.jpg?s=612x612&w=0&k=20&c=LVkPMBiZas8zxBPmhEApCv3UiYjcbYZJsO-CVQjAJeU={% endif %}" alt="publicatie-foto" width="100p">
        </article>
    {% endfor %}
    
</div>
</section>

